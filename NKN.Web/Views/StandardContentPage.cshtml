@inherits UmbracoViewPage<StandardContentPageViewModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
    Standard Page
    @foreach (var item in Model.Items)
    {
        Html.RenderAction<NestedContentController>(c => nameof(c.Index), new { viewModel = item });
    }
    </div>
</body>
</html>
