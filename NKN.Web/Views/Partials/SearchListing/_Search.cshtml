@inherits UmbracoViewPage<NKN.Core.ViewModels.Partials.Listing.ListingViewModel<NKN.Core.ViewModels.Partials.Listing.SearchResultsItemViewModel>>
	
@if (Model.Items != null && Model.Items.Any())
{
	foreach (var item in Model.Items)
	{
		<h3>@item.Title</h3>
		<p>@item.Summary</p>
		<a href="@item.Url">more...</a>
		<hr/>
	}
}
