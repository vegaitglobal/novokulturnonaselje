@inherits UmbracoViewPage<HomeViewModel>

@if (Model.ContentBlocks != null && Model.ContentBlocks.Any())
{
	foreach (var item in Model.ContentBlocks)
	{
		Html.RenderAction<NestedContentController>(c => nameof(c.Index), new { viewModel = item });
	}
}
